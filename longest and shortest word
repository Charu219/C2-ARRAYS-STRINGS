#include <iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    
    string arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    string  ans="";
    int val=0;
    for(int i=0;i<n;i++){
        string word=arr[i];
        if(word.length()>val){
            ans=word;
            val=word.length();
        }
    }
    cout<<ans;
    cout<<"\n";
    
    int val1=arr[0].length();
    string ans1="";
    for(int i=1;i<n;i++){
        string word=arr[i];
        if(word.length()<val1){
            ans1=word;
            val1=word.length();
            
        }
    }
    
    cout<<ans1;
    
    
}
