#include <iostream>
using namespace std;
int main(){
  string word1;
  string word2;
  
  cin>>word1;
  cin>>word2;
  
  for(int i=0;i<word1.length();i++){
      bool found=false;
      for(int j=0;j<word2.length();j++){
          if(i!=j && word1[i]==word2[j] && !found){
              found=true;
          }
      }
  
      if(!found){
      cout<<"not anagram";
      return 0;
  }
  }
  cout<<"anagram";
  return 0;

    
}
