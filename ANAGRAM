#include <iostream>
using namespace std;
int main(){
  string word1;
  string word2;
  
  cin>>word1>>word2;
  
  if(word1.length() != word2.length()){
        cout << "not anagram";
        return 0;
    }
    
    bool used[word2.length()]={false};
    
    for(int i=0;i<word1.length();i++){
        bool found=false;
        for(int j=0;j<word2.length();j++){
            if(word1[i]==word2[j] && !used[j]){
                used[j]=true;
                found=true;
                break;
            }
        }
        if(!found){
               cout << "not anagram";
                return 0;
        }
    }
    cout<<"anagram;";

    return 0;
}
