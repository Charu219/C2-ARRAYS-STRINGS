#include <iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int arr[n];
    
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    
    for(int i=n-1;i>=0;i--){
        if(arr[i]<9){
            arr[i]++;
            break;
        }
        else{
            arr[i]=0;
        }
    }
    
    if(arr[0]==0){
        cout<<1;
        for(int i=0;i<n;i++){
            cout<<0;
        }
    }
    
    else{
        for(int i=0;i<n;i++){
            cout<<arr[i];
        }
    }
}
