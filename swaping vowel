#include <iostream>
#include <string>
using namespace std;
int main(){
    string s;
    cin>>s;
    
    int i=0;
    int j=s.length()-1;
    
    while(i<j){
        while(i<j && !(s[i]=='a'||s[i]=='e'||s[i]=='i'|| s[i]=='o' || s[i]=='u')){
            i++;
        }
         while(i<j && !(s[j]=='a'||s[j]=='e'||s[j]=='i'|| s[j]=='o' || s[j]=='u')){
            j--;
        }
        
        char temp=s[i];
        s[i]=s[j];
        s[j]=temp;
        
        i++;
        j--;
    }
    
    cout<<s;
}
