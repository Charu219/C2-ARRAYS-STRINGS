#include <iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    
    if(n==1){
        cout<<1;
        return 0;
    }
    
    if(n==2){
        cout<<2;
        return 0;
    }
    int prev1=1;
    int prev2=2;
    int curr;
    
    for(int i=3;i<=n;i++){
        curr=prev1+prev2;
        prev1=prev2;
        prev2=curr;
    }
    cout<<curr;
}
