#include <iostream>
#include <string>
using namespace std;

int main() {
    string str;
    cin >> str;

    for (int i = 0; i < str.length(); i += 2) {
        char ch = str[i];
        int count = str[i + 1] - '0';

        for (int j = 0; j < count; j++) {
            cout << ch;
        }
    }

    return 0;
}
