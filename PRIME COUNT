#include <iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int arr[n];
    
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    int count=0;
    for(int i=0;i<n;i++){
        if(arr[i]==2 || arr[i]==3 || arr[i]==5 || arr[i]==7){
            count++;
        }
        else{
            if(arr[i]%2!=0 && arr[i]%3==0 && arr[i]%5==0 && arr[i]%7==0){
                count++;
            }
        }
    }
    cout<<count;
    
}
